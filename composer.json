{
    "name": "masakik/supervisor",
    "description": "Generate Supervisor config for monitoring Laravel worker process",
    "keywords": ["laravel", "supervisor", "config", "generator", "command"],

    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Zamroni",
            "email": "halo@matriphe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matriphe\\Supervisor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matriphe\\Tests\\Supervisor\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matriphe\\Supervisor\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
